{
  "name": "@plasmohq/parcel-transformer-svelte3",
  "type": "module",
  "version": "0.3.2",
  "description": "Plasmo Parcel Transformer for Svelte3",
  "files": [
    "dist"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsup src/index.ts --format esm,cjs --minify --clean",
    "dev": "tsup src/index.ts --sourcemap --watch"
  },
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "homepage": "https://docs.plasmo.com/",
  "engines": {
    "parcel": ">= 2.7.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PlasmoHQ/plasmo.git"
  },
  "devDependencies": {
    "@plasmo/config": "workspace:*",
    "svelte": "3.55.0",
    "tsup": "6.5.0"
  },
  "dependencies": {
    "@parcel/core": "2.8.2",
    "@parcel/diagnostic": "2.8.2",
    "@parcel/plugin": "2.8.2",
    "@parcel/source-map": "2.1.1",
    "@parcel/utils": "2.8.2",
    "@plasmo/utils": "workspace:*",
    "bundle-require": "3.1.2",
    "esbuild": "0.16.12"
  }
}
